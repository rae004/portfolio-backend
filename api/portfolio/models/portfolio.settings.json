{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "name": "portfolio"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Headline": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "Headline"
    }
  }
}
